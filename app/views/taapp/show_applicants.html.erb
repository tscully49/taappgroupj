<% provide(:title, 'All applications') %>


<h2>All Applicants</h2>
<head>
<style>
table, th, td {
    border: 1px solid black;
    border-collapse: collapse;
}
th, td {
    padding: 5px;
}
th {
    text-align: left;
}
table#t01 tr:nth-child(even) {
    background-color: #eee;
}
table#t01 tr:nth-child(odd) {
   background-color:#fff;
}
table#t01 th	{
    background-color: black;
    color: white;
}
</style>
</head>
<ul class="users">
  
    <table id="t01">
    <tr>                          
    <th> FistName </th>
    
    <th> LastName </th>
    
    <th> StudentID </th>
    
   
    </tr>
      <% @users.each do |user| %>
     
      <tr>
      <td><%= link_to user.first_name %></td>
      <td><%= link_to user.last_name %></td>
      <td><%= link_to user.student_id %></td>
      </tr>
   
  <% end %>
    </table> 


</ul>

