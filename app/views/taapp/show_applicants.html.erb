<% provide(:title, 'All applications') %>
<h1>All users</h1>

<%#= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%#= gravatar_for user, size: 50 %>
      <%= link_to user.first_name, user %>
      <%= link_to user.last_name, user %>
    </li>
  <% end %>


</ul>

<%#= will_paginate %>