<%#<% provide(:title, 'Create Professor Account') %>


    <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
    <html xmlns="http://www.w3.org/1999/xhtml">
    <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>COMPUTER SCIENCE DEPARTMENT</title>
    
    
    <link rel="stylesheets" type="text/css" href="log.css.scss" media="all">
    
    <script type="text/javascript" src="view.js"></script>
    <script type="text/javascript" src="calendar.js"></script>
    </head>
    <body id ="main_body">
    
    <div class="container">
  
    <%= form_for @admins, :url => url_for(:controller => 'admin', :action => 'create') , :html => {:id => "signup"} do |f| %>
        <div class="header">
        <h3>Create Professors Account</h3>
      </div>
      
      <div class="sep"></div>
    
      <div class="form-horizontal center">
        <div class="form-group">
          <%= f.label :professor_Name, :class => 'control-label' %>
          <!--<div class="col-md-12">-->
          <%= f.text_field :admin_name , :class => "form-control"%>
         <!--</div>-->
        </div>
      
      <div class="form-group">
          <%= f.label :mizzou_Email, :class => 'control-label' %>
          <!--<div class="col-md-12">-->
          <%= f.text_field :miz_email , :class => "form-control"%>
         <!--</div>-->
        </div>

        <div class="form-group">
          <%= f.label :Password, :class => 'control-label' %>
          <!--<div class="col-md-12">-->
          <%= f.text_field :password , :class => "form-control"%>
         <!--</div>-->
        </div>
        
      
        <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="col-md-4 col-sm-12 col-xs-12">
                  <%= link_to "Go Back", "/", :class => "btn btn-default", :method => :get %>
                </div>
                <div class="col-md-4 col-md-offset-1 col-sm-4 col-sm-offset-2 col-xs-4 col-xs-offset-2">
                  <%= f.submit "Create Professor account", class: "btn btn-primary" %>
                </div>
                </div>
              </div> <!-- END form-horizontal center DIV -->
            <% end %>
        </div>
        
        